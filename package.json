{
    "name": "squid-uniswap",
    "private": true,
    "scripts": {
        "build": "rm -rf lib && tsc",
        "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
        "db:migrate": "npx squid-typeorm-migration apply",
        "processor:start": "node lib/processor.js",
        "query-node:start": "squid-graphql-server",
        "lint": "eslint . --ext .ts --ignore-path .gitignore --ignore-pattern src/abi/**/*  --fix"
    },
    "dependencies": {
        "@subsquid/big-decimal": "^0.0.0",
        "@subsquid/evm-processor": "^0.2.4",
        "@subsquid/evm-typegen": "2.0.2",
        "@subsquid/file-store": "^0.0.0",
        "@subsquid/file-store-parquet": "^0.1.0",
        "@subsquid/file-store-s3": "^0.0.1",
        "dotenv": "^16.0.0",
        "ethers": "^5.6.4"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "typescript": "~4.6.2"
    }
}
